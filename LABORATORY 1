        "class Polygons:\n",
        "    def __init__(self, name: str, num_sides: int, area: float):\n",
        "        self.__name = name\n",
        "        self.__num_sides = num_sides\n",
        "        self.__area = area\n",
        "\n",
        "    def set_name(self, name: str):\n",
        "        self.__name = name\n",
        "\n",
        "    def set_num_sides(self, num_sides: int):\n",
        "        self.__num_sides = num_sides\n",
        "\n",
        "    def set_area(self, area: float):\n",
        "        self.__area = area\n",
        "\n",
        "    def get_name(self) -> str:\n",
        "        return self.__name\n",
        "\n",
        "    def get_num_sides(self) -> int:\n",
        "        return self.__num_sides\n",
        "\n",
        "    def get_area(self) -> float:\n",
        "        return self.__area\n",
        "\n",
        "polygon1 = Polygons(\"Triangle\", 3, 15.5)\n",
        "\n",
        "print(\"Name:\", polygon1.get_name())\n",
        "print(\"Sides:\", polygon1.get_num_sides())\n",
        "print(\"Area:\", polygon1.get_area())\n",
        "\n",
        "polygon1.set_name(\"Square\")\n",
        "polygon1.set_num_sides(4)\n",
        "polygon1.set_area(20.0)\n",
        "\n",
        "print(\"Updated Name:\", polygon1.get_name())\n",
        "print(\"Updated Sides:\", polygon1.get_num_sides())\n",
        "print(\"Updated Area:\", polygon1.get_area())\n"
      ]
    }
