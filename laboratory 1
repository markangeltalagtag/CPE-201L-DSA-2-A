        "class Polygons:\n",
        "    def __init__(self, name: str, sides: int, area: float):\n",
        "        self._name = name\n",
        "        self._sides = sides\n",
        "        self._area = area\n",
        "\n",
        "    def set_name(self, name: str):\n",
        "        self._name = name\n",
        "\n",
        "    def set_sides(self, sides: int):\n",
        "        self._sides = sides\n",
        "\n",
        "    def set_area(self, area: float):\n",
        "        self._area = area\n",
        "\n",
        "    def get_name(self) -> str:\n",
        "        return self._name\n",
        "\n",
        "    def get_sides(self) -> int:\n",
        "        return self._sides\n",
        "\n",
        "    def get_area(self) -> float:\n",
        "        return self._area\n",
        "\n",
        "polygon1 = Polygons(\"Triangle\", 3, 15.5)\n",
        "\n",
        "print(\"Name:\", polygon1.get_name())\n",
        "print(\"Sides:\", polygon1.get_sides())\n",
        "print(\"Area:\", polygon1.get_area())\n",
        "\n",
        "polygon1.set_name(\"Square\")\n",
        "polygon1.set_sides(4)\n",
        "polygon1.set_area(25.0)\n",
        "\n",
        "print(\"\\nUpdated Polygon:\")\n",
        "print(\"Name:\", polygon1.get_name())\n",
        "print(\"Sides:\", polygon1.get_sides())\n",
        "print(\"Area:\", polygon1.get_area())\n"
